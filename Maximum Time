public class Solution{
  public string MaxTime(string time){
    char[] arr = time.ToCharArray();
    if(arr[0]=='?'){
      arr[0]=(arr[1]<=3 || arr[1]=='?')?'2':'1';
    }
    if(arr[1]=='?'){
      arr[1]=(arr[0]=='2')?'3':'9';
    }
    arr[3]=(arr[3]=='?')?'5':arr[3];
    arr[4]=(arr[4]=='?')?'9':arr[4];
  }
  return arr;
}
