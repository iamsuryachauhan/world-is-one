public class Solution {
    public int[] TwoSum(int[] nums, int target) {
        int arrLength = nums.Length;
        int[] index = new int[2];
        for(int i=0;i<arrLength;i++){
            for(int j=i+1;j<arrLength;j++){
                if(nums[i]+nums[j]==target){
                   index[0]=i;
                    index[1]=j;
                    break;
                    
                }
            }
        }
         return index;
    }
}
